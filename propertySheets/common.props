<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <BuildDir>$(SolutionDir)build\</BuildDir>
    <OutputDir>$(BuildDir)$(PlatformTarget)\$(Configuration)\</OutputDir>
    <InterDir>$(BuildDir)$(ProjectName)\$(PlatformTarget)\$(Configuration)\</InterDir>
  </PropertyGroup>
  <PropertyGroup />
  <ItemDefinitionGroup />
  <ItemGroup>
    <BuildMacro Include="BuildDir">
      <Value>$(BuildDir)</Value>
    </BuildMacro>
    <BuildMacro Include="OutputDir">
      <Value>$(OutputDir)</Value>
    </BuildMacro>
    <BuildMacro Include="InterDir">
      <Value>$(InterDir)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>